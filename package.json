{
  "name": "pg-sourcerer-monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "prestart": "cd packages/pg-sourcerer && bun run build",
    "db:ensure": " cd packages/example && bun run db:ensure",
    "start": "cd packages/example && bun start",
    "generate": " cd packages/example && bun run generate",
    "test": " cd packages/pg-sourcerer && bun run test",
    "predev": "bun run db:ensure",
    "dev": "mprocs 'bun run build:watch' 'bun run generate'",
    "build": "cd packages/pg-sourcerer && bun run build",
    "build:watch": "cd packages/pg-sourcerer && bun run build:watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "bun --workspaces run typecheck"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalog": {
      "effect": "^3.19.14",
      "@effect/cli": "^0.73.0",
      "@effect/platform": "^0.94.1",
      "@effect/platform-bun": "^0.87.0",
      "@effect/platform-node": "^0.104.0",
      "@effect/vitest": "^0.27.0",
      "pg-introspection": "^1.0.0-rc.2",
      "pg": "^8.16.0",
      "postgres": "^3.4.7",
      "@types/pg": "^8.15.4",
      "vitest": "^3.2.4",
      "typescript": "^5.7.2",
      "@types/bun": "^1.1.14"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "mprocs": "^0.8.2",
    "typescript-eslint": "^8.52.0"
  }
}
